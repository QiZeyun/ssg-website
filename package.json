{
  "name": "ssg-website",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "description": "Static Site Generation website with SEO and analytics",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "export": "next build && next export",
    "validate": "tsx scripts/validate-build.ts",
    "preview": "tsx scripts/preview-build.ts",
    "build:preview": "pnpm build && pnpm validate && pnpm preview",
    "pre-commit:check": "tsx scripts/pre-commit-check.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.33",
    "serve": "^14.2.5",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
